{
  "name": "angular-curve",
  "repo": "kenhkan/angular-curve",
  "version": "0.0.0",
  "main": [
    "public/app.js",
    "public/app.css"
  ],
  "ignore": [
    "node_modules",
    "bower_components",
    "components"
  ],
  "dependencies": {
    "angular": "~1.2.0",
    "lazy.js": "~0.3.2",
    "angular-mocks": "~1.2.0"
  },
  "curve": {
    "templates": {
      "extensions": "html|hbs|eco|jade"
    },
    "javascripts": {
      "order": {
        "before": [
          "bower_components/lazy.js/lazy.js",
          "bower_components/angular/angular.js"
        ]
      },
      "splitTo": {
        "_dev/test_lib": [
          "bower_components/angular-mocks/angular-mocks.js"
        ]
      }
    }
  }
}
