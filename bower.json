{
  "name": "angular-curve",
  "repo": "kenhkan/angular-curve",
  "version": "0.0.0",
  "main": [
    "public/app.js",
    "public/templates.js",
    "public/app.css"
  ],
  "ignore": [
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "angular": "~1.2.0",
    "lazy.js": "~0.3.2",
    "angular-mocks": "~1.2.0",
    "console-polyfill": "~0.1.0"
  },
  "curve": {
    "javascripts": {
      "splitTo": {
        "vendor_dev": [
          "bower_components/console-polyfill/index.js"
        ],
        "vendor_test": [
          "bower_components/angular-mocks/angular-mocks.js"
        ]
      }
    }
  }
}
